{
	"name": "z751",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"ng": "ng",
		"build": "ng build --prod",
		"start": "ng serve --open",
		"test": "ng test",
		"lint": "tslint ./src/**/*.ts -t verbose",
		"e2e": "ng e2e",
		"start:en": "ng serve --aot --i18nFile=src/locale/messages.en.xlf --i18nFormat=xlf --locale=en",
		"build:en": "ng build --prod --i18nFile=src/locale/messages.en.xlf --i18nFormat=xlf --locale=en",
		"extract": "ng xi18n --outputPath=src/locale",
		"build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
		"serve:ssr": "cd dist && node server.js",
		"build:client-and-server-bundles": "ng build --prod && ng run z751:server:production",
		"webpack:server": "webpack --config webpack.server.config.js --progress --colors",
		"render": "npm run build:ssr && npm run serve:ssr"
	},
